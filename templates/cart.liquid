            e.preventDefault();
            var id = $(this).data('current-id');
            var qty = $(this).data('current-qty')*1 - 1;
            var new_id = $('#editLineItem .product-single__variants').val();
            //theme.cart.changeItem(id,qty);
            $('#editLineItem .payment-buttons').prepend('<div id="" class="button--loading"><span>loading</span></div>')
            $('#editLineItem .add-to-cart').hide();
           
            $.ajax({
              type: 'POST',
              url: '/cart/add.js',
              data: {items: [
                {
                  id: new_id,
                  quantity: 1
                }
              ]},
              dataType: 'json'
            })
            .then(function(cart) {
                $.ajax({
                  type: 'POST',
                  url: '/cart/change.js',
                  data: 'quantity=' + qty + '&id=' + id,
                  dataType: 'json'
            	}) 
                    .then(function(cart) {
					  $('#editLineItem .button--loading').remove();
                      $('#editLineItem .add-to-cart').show();
                      $('#editLineItem').hide();
                      updateCartPage(function(){
                         
                      });
