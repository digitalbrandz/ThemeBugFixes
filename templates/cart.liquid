          	var cart_tabs_content 	= '.cart-tab_content';
            $(cart_tabs).click(function(){
            	var target = '#' + $(this).attr('aria-controls');
              	$(cart_tabs_content).addClass('hide');
                $(target).removeClass('hide');
            	$(cart_tabs).removeClass(activeClass);
            	$(this).addClass(activeClass)
            })
            function updateCartPage(callback){
            	$.ajax({
                  type: 'GET',
                  url: '/cart'
            	})
                .then(function(data) {
                  var newDetails = $(data).find('#cart-page__details');
                  var newTotal = $(data).find('#cart-level__subtotal');
                  var newTab = $(data).find('#CartLabel');
                  $('#cart-page__details').html($(newDetails).html());
                  $('#cart-level__subtotal').html($(newTotal).html());
                  $('#CartLabel').html($(newTab).html());
				  callback();
                })
            }
          	function updateShareLink(cart){
              var link = '{{shop.url}}/cart/';
              for(var i=0;i<cart.items.length;i++){
                link += cart.items[i].variant_id + ':' + cart.items[i].quantity + ',';
              }
              shareLink = link;
            	console.log(123,link);
            }
            $(document).on('click','.line-item__edit',function(){
                console.log($(this).val())
            });
          	$(document).on('change','.line-item__quantity-input',function(){
              var qty =  $(this).val();
              var line_item = $(this).closest('tr.cart-content__item');
              var id = line_item.data('variant-id');
              //theme.cart.changeItem(id,qty);
              $('#loading_checkout').show();
              $('#submit_checkout').hide();
              $.ajax({
                  type: 'POST',
                  url: '/cart/change.js',
                  data: 'quantity=' + qty + '&id=' + id,
                  dataType: 'json'
            	})
                .then(function(cart) {
                    //return cart;
                    updateCartPage(function(){
						$('#loading_checkout').hide();
              			$('#submit_checkout').show();
                    });
                	updateShareLink(cart);
                })
              
            });
          shareLink = '{{shop.url}}/cart/{% for line_item in cart.items %}{{ line_item.variant_id }}:{{ line_item.quantity }},{% endfor %}';
          	
          function shareCart(){
          	var t = 'SHARE YOUR CART';
          	var e = shareLink;
            var shareCartHtml = `
   <div class="share-cart-modal" id="shareCart">
