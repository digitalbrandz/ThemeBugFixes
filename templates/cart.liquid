<div class="page-width page-content">

  {% include 'breadcrumbs' %}

  <div class="grid">
    <div class="">
      {% if cart.item_count > 0 %}
      	<div class="cart-page">
          <div class="cart-page__header">
            <div class="header__continue-shopping">
              <span class="cart-page__continue-separator"></span>
              <a href="/" class="cart-page__continue">{{ 'cart.label.continue' | t }}</a>
            </div>
            <h1 class="cart-page__header-title">{{ 'cart.general.title' | t }}</h1>
          </div>

          <div class="cart-page-tabs" role="tablist" aria-label="Shopping Bag">
            <button id="CartLabel" class="cart-page-tabs__button cart-page-tabs__button--active" aria-controls="CartTab" >
              {{ 'cart.label.your_bag' | t }} (<span data-cart-item-count>{{ cart.item_count }}</span>)
            </button>
            <button id="RecentlyViewedLabel" class="cart-page-tabs__button" aria-controls="RecentlyViewedTab">
              {{ 'cart.label.recently_viewed' | t }}
            </button>
          </div>
          <div id="CartTab" class="cart-tab_content">
            <form action="/cart" method="post" novalidate class="cart">
              <div class="cart-page__container" data-cart-wrapper >
				<div id="cart-page__details" >
                  <div class="cart-page__mobile-checkout">

                    <div class="cart-level__subtotal">
                      <div class="cart-level__subtotal-heading">
                        <div>
                          <span class=>{{ 'cart.label.subtotal' | t }}</span>
                          <span class="checkout-subtotal__item-count"> (<span data-cart-item-count>{{ cart.item_count }}</span> items)</span>
                        </div>
                      </div>
                      <div class="cart-level__subtotal-info">
                        <span class="checkout-subtotal__total" data-cart-subtotal>
                              <span class=money>{{ cart.total_price | money }}</span>
                        </span>
                      </div>
                    </div>


                    <input type="submit" name="checkout" class="cart__submit" value="Checkout"/>
                  </div>


                  <div class="cart-page__content">
                    <table class="cart-content__table">
                      <thead class="cart-content__table-header">
                        <tr>
                          <th class="table-header__info">{{ 'cart.label.item' | t }}</th>
                          <th class="table-header__quantity">{{ 'cart.label.quantity' | t }}</th>
                          <th class="table-header__price">{{ 'cart.label.price' | t }}</th>
                        </tr>
                      </thead>
                      <tbody data-cart-line-items>
                        {% for line_item in cart.items %}
                        <tr class="cart-content__item item"
                            data-cart-item data-cart-item-key="{{ line_item.key }}"
                            data-variant-id="{{ line_item.variant_id }}"
                            data-product-id="{{ line_item.product_id }}"
                            data-cart-item-index="{{forloop.index}}"
                            data-cart-item-quantity="{{ line_item.quantity }}"
                            
                            >
                            <td class="line-item__info">
                              <a class="line-item__product-image" href="{{line_item.variant.url}}">
                                {{ line_item.image | img_url: '100x100' | img_tag }}
                              </a>
                              <a class="product-title" href="{{line_item.variant.url}}" >{{ line_item.product.title }}</a>

                                <ul class="cart-page-option-list">
                                  {% for option in line_item.product.options %}
                                    <li>{{option}}: {{line_item.variant.options[forloop.index0]}}</li>
                                  {% endfor %}
                                </ul>


                              <div class="line-item__edit">
                                <svg width="12" height="12" viewBox="0 0 24 24" title="Pencil" fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true" focusable="false" role="presentation">
                                  <path d="M9.2 19.655l-6.7 1.91 1.92-6.71 11.46-11.46a3.39 3.39 0 0 1 4.79 4.79L9.2 19.655zm5.25-14.82l4.79 4.79m-14.82 5.24l4.79 4.79" />
                                </svg>
                                <span data-cart-item-edit="{{ line_item.product.url }}" data-cart-item-edit-key="{{ line_item.variant_id }}" data-cart-item-edit-qty="{{ line_item.quantity }}">
                                  {{ 'cart.label.edit' | t }}
                                </span>
                              </div>
                            </td>


                            <td class="line-item__quantity">
                              <div>
                                <label for="updates_large_12203936350332:395e8f77f74cec239330a6d4afc55c9a" class="line-item__quantity-label" data-quantity-label-desktop>{{ 'cart.label.quantity' | t }}</label>
                                <input id="updates_large_12203936350332:395e8f77f74cec239330a6d4afc55c9a" class="line-item__quantity-input" type="number"
                                  name="updates[]" value="{{ line_item.quantity }}" min="0" pattern="[0-9]*">
                              </div>

                            </td>

                            <td class="line-item__price">
                              <div class="grid-item__price">
                                <div>
                                  <dl class="line-item__price-list final-sale--cart">
                                    <div  data-cart-item-regular-price-group>

                                      <dd class="line-item__price-list-definition" data-cart-item-regular-price>
                                        <span class=money>{{ line_item.final_line_price | money }}</span>
                                      </dd>
                                    </div>
                                  </dl>
                                </div>
                              </div>

                              <div class="line-item__adjustments">
                                <a class="line-item__remove" href="/cart/change?line={{forloop.index}}&quantity=0" >Remove</a>
                              </div>


                              <div class="line-item__quantity-mobile">
                                <div>
                                  <label for="updates_12203936350332:395e8f77f74cec239330a6d4afc55c9a" class="line-item__quantity-label" data-quantity-label-mobile>QTY</label>
                                  <input id="updates_12203936350332:395e8f77f74cec239330a6d4afc55c9a" class="line-item__quantity-input" type="number"
                                    value="{{ line_item.quantity }}" min="0" pattern="[0-9]*">
                                </div>

                              </div>
                            </td>
                          </tr>
                        {% endfor %}
                      </tbody>
                    </table>
                    <div class="cart-content__share">
                      <svg width="11" height="12" fill="none" stroke-linecap="round" stroke-linejoin="round">
                        <path d="M.5 5.5v4.8c0 .663.56 1.2 1.25 1.2h7.5c.69 0 1.25-.537 1.25-1.2V5.5m-2-2l-3-3-3 3m3-3v7"/>
                      </svg>
                      <span class="cart-content__share-button">{{ 'cart.label.share_cart' | t }}</span>
                    </div>

                  </div>
              	</div>
                <div class="cart-page__summary">

                    <div class="cart-page__discounts">
                      <input type="hidden" id="cart-page__discount-value" name="_discount" />
                      <input type="text" class="cart-page__discounts__input" name="discount"data-cart-discount placeholder=" "/>
                      <label class="cart-page__discounts__label">Discount code</label>
                    </div>


                  <div class="cart-page__checkout">
                    <div class="cart-level__discounts" hidden data-cart-level-discount>
                      <div class="cart-level__discounts-heading">
                        <span>See discount in checkout</span>
                      </div>
                      
                    </div>

                    <div id="cart-level__subtotal" class="cart-level__subtotal">
                      <div class="cart-level__subtotal-heading">
                        <div>
                          <span>{{ 'cart.label.subtotal' | t }}</span>
                          <span class="checkout-subtotal__item-count"> (<span data-cart-item-count>{{ cart.item_count }}</span> items)</span>
                        </div>
                      </div>
                      <div class="cart-level__subtotal-info">
                            <span class="checkout-subtotal__pre-discount hide" data-cart-pre-discount></span>
                        <span class="checkout-subtotal__total" data-cart-subtotal><span class=money>{{ cart.total_price | money }}</span></span>
                      </div>
                    </div>

                    
                    <div id="loading_checkout" class="button--loading"><span>loading</span></div>
                    <input id="submit_checkout" type="submit" name="checkout" class="cart__submit" value="Checkout">
                    {% if additional_checkout_buttons and settings.cart_additional_buttons %}
                      <div class="cart-page__separator">
                        <span>You can also checkout with</span>
                      </div>
                      <div class="additional_checkout_buttons">{{ content_for_additional_checkout_buttons }}</div>
                    {% endif %}
                    
                    </div>
                </div>

              </div>
              
            </form>

            <div class="cart-page__empty-content empty-page-content text-center" hidden data-empty-page-content>
              <h3 class="empty-content__title">Your shopping bag is empty.</h3>
              <a href="/" class="empty-content__continue">Continue shopping</a>
            </div>
          </div>

          <div id="RecentlyViewedTab" class="cart-tab_content hide">
            <div class="cart-page__recommendations">
              <div id="recently_viewed" class="cart-page__recently-viewed">
                <div data-recently-viewed>
                  {%- assign grid_item_width = 'small--one-half medium-up--one-fifth' -%}
                  <div
                    data-subsection
                    data-section-id=""
                    data-section-type="recently-viewed"
                    data-product-handle=" "
                    data-recent-count="5"
                    data-grid-item-class="{{ grid_item_width }}">
                    <div id="RecentlyViewed-{{ section.id }}"></div>
                  </div>
                  
                </div>
              </div>
            </div>
          </div>
   		</div>
      
      <!----------------------------------------------------------------------------------------->
       
      {% else %}
        <header class="section-header section-header--404">
          <h1 class="section-header__title">{{ 'cart.general.title' | t }}</h1>
          <div class="rte text-spacing">
            <p>{{ 'cart.general.empty' | t }}</p>
            <p >{{ 'cart.general.continue_browsing_html' | t }}</p>
          </div>
        </header>
      {% endif %}
    </div>
  </div>
</div>
<div class="edit-line-item aos-animate" id="editLineItem">             
   <div class="edit-line-item__overlay-fallback" tabindex="-1" data-a11y-dialog-hide></div>             
   <div class="edit-line-item__dialog" role="dialog">                  
     <button class="edit-line-item__close-button" type="button" data-a11y-dialog-hide aria-label="Close this dialog window">                  
   		<span>Close</span>                 
     </button>
     <div id="edit-line-item__content"></div>
   </div>           
</div>
