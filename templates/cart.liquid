              const e = document.querySelector(".share-cart__url");
                    e.select();
                    e.focus();
                    e.setSelectionRange(0, e.value.length);
            	document.execCommand("copy");
                    const t = document.execCommand("copy")
                      , n = document.querySelector("[data-copy-cart]");
                    t ? (n.textContent = "Copied!",
                    setTimeout(()=>{
                        n.textContent = "Copy URL"
                    }
                    , 3e3)) : (n.textContent = "Press CTRL + C to copy",
                    setTimeout(()=>{
                        n.textContent = "Copy URL"
                    }
                    , 3e3))	
          })
          $(document).on('click','.share-cart-modal__close-button, .share-cart-modal__overlay-fallback',function(){
              $('#shareCart').remove();          
          })
          $(document).on('click','[data-cart-item-edit]',function(){
              var handle = $(this).data('cart-item-edit');
              var id = $(this).data('cart-item-edit-key');
              var qty = $(this).data('cart-item-edit-qty');
              $.ajax({
                  type: 'GET',
                  url: handle + '?variant=' + id + '&section_id=product-template'
            	})
                .then(function(data) {
                   $('#editLineItem #edit-line-item__content').html(data);
                   $('#editLineItem').show();
                   $('#editLineItem form').attr('data-current-id',id);
                	$('#editLineItem form').attr('data-current-qty',qty);
                	//sections.register('product', theme.Product);
        			sections.register('product-template', theme.Product);
                	//console.log('prodcuttamplte',data);
                })
          })
          $(document).on('click','.edit-line-item__close-button, .edit-line-item__overlay-fallback',function(){
              $('#editLineItem').hide();          
          })
